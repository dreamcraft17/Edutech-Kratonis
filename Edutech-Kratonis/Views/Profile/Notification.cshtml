
@{
    ViewBag.Title = "Notification";
    Layout = "~/Views/Shared/_LayoutSide.cshtml";
}

@{
    var currentDate = DateTime.Now.ToString("dd MMMM yyyy");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <style>
        .user-icon {
            font-size: 80px;
        }

        .profile-container {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
      /*      min-height: calc(100vh - 70px);*/
            text-align: center;
        }

        .profile-header {
            margin-bottom: 20px;
        }

        .content {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

            .content .item {
                width: 150px;
                height: 100px;
                background: #e0e0e0;
                margin: 10px;
            }

        .see-more {
            margin-top: 20px;
            padding: 10px 20px;
            background-color: #f06292;
            color: white;
            border: none;
            border-radius: 5px;
            cursor: pointer;
        }
    </style>
</head>
<body>
    <div class="profile-container">
        <div class="profile-header">
          <h1>No Notification!</h1>
        </div>
    
    </div>
</body>
</html>


